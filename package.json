{
  "name": "devutils-manager",
  "displayName": "DevUtils Manager – Productivity Toolkit for Developers in VS Code",
  "description": "Manage, run, and organize your DevUtils collections directly from VS Code. Speed up your workflow with fast and accessible utilities.",
  "version": "0.2.0",
  "publisher": "abrahamdiazdev",
  "engines": {
    "vscode": "^1.105.0"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "keywords": [
    "snippets",
    "utilities",
    "code reuse",
    "functions",
    "library",
    "personal",
    "productivity",
    "developer utilities manager",
    "devtools workflow",
    "productivity tools for developers"
  ],

  "activationEvents": [],
  "main": "./dist/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/abraham-diaz/devutils-manager"
  },
  "bugs": {
    "url": "https://github.com/abraham-diaz/devutils-manager/issues"
  },
  "homepage": "https://github.com/abraham-diaz/devutils-manager#readme",
  "contributes": {
    "commands": [
      {
        "command": "devutils-manager.saveFunction",
        "title": "DevUtils: Save Function",
        "icon": "$(save)"
      },
      {
        "command": "devutils-manager.showFunctions",
        "title": "DevUtils: Show My Functions",
        "icon": "$(library)"
      },
      {
        "command": "devutils-manager.insertFunction",
        "title": "DevUtils: Insert Function",
        "icon": "$(symbol-function)"
      },
      {
        "command": "devutils-manager.configureCommand",
        "title": "DevUtils: Configure Custom Command",
        "icon": "$(settings-gear)"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "devutils-manager.saveFunction",
          "group": "9_cutcopypaste",
          "when": "editorHasSelection"
        }
      ]
    },
    "keybindings": [
      {
        "command": "devutils-manager.saveFunction",
        "key": "ctrl+alt+s",
        "mac": "cmd+alt+s",
        "when": "editorHasSelection"
      },
      {
        "command": "devutils-manager.showFunctions",
        "key": "ctrl+alt+f",
        "mac": "cmd+alt+f"
      },
      {
        "command": "devutils-manager.insertFunction",
        "key": "ctrl+alt+i",
        "mac": "cmd+alt+i",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
